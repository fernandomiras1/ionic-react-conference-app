# Fastlane Environment Variables
# Copy this to .env.local and fill with your actual values

# iOS Configuration
FASTLANE_APPLE_ID=your-apple-id@email.com
FASTLANE_TEAM_ID=YOUR_TEAM_ID
FASTLANE_ITC_TEAM_ID=YOUR_ITC_TEAM_ID
FASTLANE_PASSWORD=your-apple-password
FASTLANE_APP_IDENTIFIER=com.fernandomiras.ionicconference

# Android Configuration
ANDROID_JSON_KEY_FILE=path/to/your/google-play-api-key.json
ANDROID_PACKAGE_NAME=com.fernandomiras.ionicconference


# Slack notifications (optional)
SLACK_URL=your-slack-webhook-url

# Code Signing
MATCH_PASSWORD=your-match-password
MATCH_GIT_URL=https://github.com/yourusername/certificates

# Additional Fastlane configurations
FASTLANE_SKIP_UPDATE_CHECK=1
FASTLANE_HIDE_GITHUB_ISSUES=1
FASTLANE_DISABLE_COLORS=0

# Build configurations
FL_BUILD_NUMBER_USE_HGREV_OR_GITREV=true
PILOT_SKIP_SUBMISSION=false
PILOT_SKIP_WAITING_FOR_BUILD_PROCESSING=true
