app_identifier(ENV['FASTLANE_APP_IDENTIFIER'] || "com.fernandomiras.ionicconference") # The bundle identifier of your app
apple_id(ENV['FASTLANE_APPLE_ID'] || "your-apple-id@email.com") # Your Apple Developer Portal username

team_id(ENV['FASTLANE_TEAM_ID'] || "YOUR_TEAM_ID") # Developer Portal Team ID
itunes_connect_team_id(ENV['FASTLANE_ITC_TEAM_ID'] || "YOUR_ITC_TEAM_ID") # App Store Connect Team ID (if different)

